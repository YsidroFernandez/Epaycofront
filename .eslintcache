) => {\r\n    setPassword(event.target.value);\r\n    setOpen(false);\r\n  };  \r\n\r\n  return (\r\n\r\n    <Container maxWidth=\"lg\" className={classes.root} >\r\n      <div className={classes.paper}>\r\n        <Paper >\r\n          <form className={classes.form} noValidate autoComplete=\"off\">\r\n            <Typography variant=\"subtitle1\" className={classes.label}>\r\n              Email\r\n            </Typography>\r\n            <TextField  \r\n              id=\"email\" \r\n              label=\"Email\"\r\n              className={classes.label} \r\n              value={email} \r\n              onChange={handleChangeEmail} \r\n            />\r\n            <Typography variant=\"subtitle1\" className={classes.label}>\r\n              Password\r\n            </Typography>\r\n            <TextField id=\"password\" label=\"Password\" type=\"password\" value={password} onChange={handleChangePassword} />\r\n            <ButtonComponent color=\"primary\" action={handleLogin} />\r\n            <CustomizedSnackbars open={open} severity={severity} close={handleClose} message={message}/>\r\n          </form>\r\n        </Paper>\r\n      </div>\r\n\r\n    </Container>\r\n\r\n\r\n\r\n  );\r\n}\r\n","D:\\Documents\\programming projects\\virtual-wallet\\src\\API\\base\\index.js",[],"D:\\Documents\\programming projects\\virtual-wallet\\src\\reportWebVitals.js",[],"D:\\Documents\\programming projects\\virtual-wallet\\src\\views\\home\\home.view.js",["46"],"D:\\Documents\\programming projects\\virtual-wallet\\src\\component\\toast\\index.js",[],"D:\\Documents\\programming projects\\virtual-wallet\\src\\component\\button\\index.js",["47"],"D:\\Documents\\programming projects\\virtual-wallet\\src\\views\\dashboard\\dashboard.view.js",["48","49"],{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","replacedBy":"53"},{"ruleId":"50","replacedBy":"54"},{"ruleId":"52","replacedBy":"55"},{"ruleId":"56","severity":1,"message":"57","line":7,"column":8,"nodeType":"58","messageId":"59",